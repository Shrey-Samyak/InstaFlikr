<?xml version = "1.0" encoding = "utf-8"?>

<com.sothree.slidinguppanel.SlidingUpPanelLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:sothree="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="bottom"
    sothree:panelHeight="10dp"
    sothree:shadowHeight="4dp"
    sothree:paralaxOffset="100dp"
    tools:context=".PhotoFullscreenActivity">

    <fragment
        android:id="@+id/photoFullScreenFragment"
        android:name="com.pluralapps.instaflikr.PhotoFullscreenFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


    <fragment
        android:id="@+id/textviewTestFragment"
        android:name="com.pluralapps.instaflikr.PhotoCommentsListFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</com.sothree.slidinguppanel.SlidingUpPanelLayout>

